<?xml version='1.0' encoding='utf-8'?>
<widget id="com.agenciamagine.escpart" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="startApp">
        <param name="android-package" value="com.lampa.startapp.startApp" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>Onde Reclamar</name>
    <description>Onde Reclamar.</description>
    <author email="contato@agenciamagine.com.br" href="http://agenciamagine.com.br">Luar Faria</author>
    <content src="index.html" />
    <preference name="phonegap-version" value="cli-6.0.0" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-targetSdkVersion" value="20" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.1" />
    <plugin source="git" spec="https://github.com/vilic/cordova-plugin-tts.git" />
    <plugin name="cordova-plugin-device" spec="~1.1.3" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.0" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <icon src="icon.png" />
    <icon gap:density="ldpi" platform="android" src="res/icon/android/icon-36-ldpi.png" />
    <icon gap:density="mdpi" platform="android" src="res/icon/android/icon-48-mdpi.png" />
    <icon gap:density="hdpi" platform="android" src="res/icon/android/icon-72-hdpi.png" />
    <icon gap:density="xhdpi" platform="android" src="res/icon/android/icon-96-xhdpi.png" />
    <icon platform="blackberry" src="res/icon/blackberry/icon-80.png" />
    <icon gap:state="hover" platform="blackberry" src="res/icon/blackberry/icon-80.png" />
    <icon height="57" platform="ios" src="res/icon/ios/icon.png" width="57" />
    <icon height="114" platform="ios" src="res/icon/ios/icon@2x.png" width="114" />
    <icon height="180" platform="ios" src="res/icon/ios/icon-60@3x.png" width="180" />
    <icon height="72" platform="ios" src="res/icon/ios/icon-72.png" width="72" />
    <icon height="144" platform="ios" src="res/icon/ios/icon-72@2x.png" width="144" />
    <icon height="60" platform="ios" src="res/icon/ios/icon-60.png" width="60" />
    <icon height="120" platform="ios" src="res/icon/ios/icon-60@2x.png" width="120" />
    <icon height="76" platform="ios" src="res/icon/ios/icon-76.png" width="76" />
    <icon height="152" platform="ios" src="res/icon/ios/icon-76@2x.png" width="152" />
    <icon height="29" platform="ios" src="res/icon/ios/icon-small.png" width="29" />
    <icon height="58" platform="ios" src="res/icon/ios/icon-small@2x.png" width="58" />
    <icon height="40" platform="ios" src="res/icon/ios/icon-40.png" width="40" />
    <icon height="80" platform="ios" src="res/icon/ios/icon-40@2x.png" width="80" />
    <icon platform="webos" src="res/icon/webos/icon-64.png" />
    <icon platform="winphone" src="res/icon/windows-phone/icon-48.png" />
    <icon gap:role="background" platform="winphone" src="res/icon/windows-phone/icon-173.png" />
    <splash gap:density="ldpi" gap:qualifier="port-ldpi" platform="android" src="res/screen/android/screen-ldpi-portrait.png" />
    <splash gap:density="mdpi" gap:qualifier="port-mdpi" platform="android" src="res/screen/android/screen-mdpi-portrait.png" />
    <splash gap:density="hdpi" gap:qualifier="port-hdpi" platform="android" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash gap:density="xhdpi" gap:qualifier="port-xhdpi" platform="android" src="res/screen/android/screen-xhdpi-portrait.png" />
    <splash gap:density="ldpi" gap:qualifier="land-ldpi" platform="android" src="res/screen/android/screen-ldpi-landscape.png" />
    <splash gap:density="mdpi" gap:qualifier="land-mdpi" platform="android" src="res/screen/android/screen-mdpi-landscape.png" />
    <splash gap:density="hdpi" gap:qualifier="land-hdpi" platform="android" src="res/screen/android/screen-hdpi-landscape.png" />
    <splash gap:density="xhdpi" gap:qualifier="land-xhdpi" platform="android" src="res/screen/android/screen-xhdpi-landscape.png" />
    <splash height="480" platform="ios" src="res/screen/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" platform="ios" src="res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="1136" platform="ios" src="res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash height="1334" platform="ios" src="res/screen/ios/Splash-750x1334.png" width="750" />
    <splash height="2208" platform="ios" src="res/screen/ios/Splash-1242x2208.png" width="1242" />
    <splash height="1242" platform="ios" src="res/screen/ios/screen-ipad-landscape-2x.png" width="2208" />
    <gap:splash gap:platform="winphone" src="res/screen/windows-phone/screen-portrait.jpg" />
</widget>
